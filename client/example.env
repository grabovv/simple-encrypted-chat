# IP address of the server that the client connects to
SRV_IP=127.0.0.1

# Port number on which the server is listening
SRV_PORT=65432

# Server key used to authorize clients (must match SRV_KEY on the server)
SRV_KEY=server_key

# Client key used for local end-to-end message encryption and decryption
CLIENT_KEY=client_key
